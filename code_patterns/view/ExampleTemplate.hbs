<div class="{{cssClassPrefix}}container">
  <nav class="{{cssClassPrefix}}nav">
  {{#each navURLs}}
    <a class="{{../cssClassPrefix}}nav-item {{../jsPrefix}}NavItem" href="{{url}}">{{label}}</a>
  {{/each}}
  {{#if user}}
    <a 
      class="{{../cssClassPrefix}}nav-item {{../cssModifierPrefix}}is--logout {{../jsPrefix}}NavItem {{../jsPrefix}}LogOutItem" 
      href="{{user.logoutURL}}"
    >{{user.logoutLabel}}</a>
  {{/if}}
  </nav>
</div>